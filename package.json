{
  "name": "skylark-formio",
  "description": "A version of formio.js that ported to running on skylarkjs.",
  "version": "0.9.0",
  "keywords": [
    "skylark",
    "framework",
    "skylark-formio"
  ],
  "homepage": "https://github.com/skylark-integration/skylark-formio/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "slib build ."
  },
  "files": [
    "demo/",
    "dist/",
    "src/",
    "skins/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-integration/skylark-formio.git"
  },
  "bugs": {
    "url": "https://github.com/skylark-integration/skylark-formio/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-bootstrap3": "git+https://github.com/skylark-integration/skylark-bootstrap3.git",
    "skylark-choices": "git+https://github.com/skylark-integration/skylark-choices.git",
    "skylark-dragula": "git+https://github.com/skylark-integration/skylark-dragula.git",
    "skylark-i18next": "git+https://github.com/skylark-integration/skylark-i18next.git",
    "skylark-jquery": "git+https://github.com/skylark-integration/skylark-jquery.git",
    "skylark-langx": "git+https://github.com/skylarklangx/skylark-langx.git",
    "skylark-lodash": "git+https://github.com/skylark-integration/skylark-lodash.git",
    "skylark-moment": "git+https://github.com/skylark-integration/skylark-moment.git",
    "skylark-popper": "git+https://github.com/skylark-integration/skylark-popper.git",
    "skylark-textmask": "git+https://github.com/skylark-integration/skylark-textmask.git"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css",
          "ejs"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "all": true
      }
    }
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
